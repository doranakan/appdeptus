{
  "expo": {
    "name": "appdeptus",
    "slug": "appdeptus",
    "version": "0.0.1",
    "scheme": "appdeptus",
    "orientation": "portrait",
    "icon": "./assets/resources/icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/resources/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#18212F"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.appdeptus"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/resources/adaptive-icon.png",
        "backgroundColor": "#18212F"
      },
      "package": "com.appdeptus"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/resources/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/AmericanText.ttf"]
        }
      ],
      [
        "expo-asset",
        {
          "assets": [
            "./assets/resources/icon_mono.png",
            "./assets/resources/splash.png",

            "./assets/resources/armies/adepta_sororitas.jpg",
            "./assets/resources/armies/adeptus_astartes.jpg",
            "./assets/resources/armies/aeldari.jpg",
            "./assets/resources/armies/astra_militarum.jpg",
            "./assets/resources/armies/chaos_space_marines.jpg",
            "./assets/resources/armies/tyranids.jpg",

            "./assets/resources/bkg/army_builder.jpg",
            "./assets/resources/bkg/games.jpg",
            "./assets/resources/bkg/sign_in.jpg",
            "./assets/resources/bkg/user_profile.jpg"
          ]
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2fe3bf31-db8b-4049-a6da-5fe5002ecdc7"
      }
    }
  }
}
