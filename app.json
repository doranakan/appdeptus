{
  "expo": {
    "name": "appdeptus",
    "slug": "appdeptus",
    "version": "0.0.1",
    "scheme": "com.appdeptus",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#252933"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.appdeptus"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#252933"
      },
      "package": "com.appdeptus"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-asset",
        {
          "assets": [
            "./assets/resources/adepta-sororitas-bkg.png",
            "./assets/resources/adeptus-astartes-bkg.png",
            "./assets/resources/adeptus-mechanicus-bkg.png",
            "./assets/resources/aeldari-bkg.png",
            "./assets/resources/army-builder-bkg.png",
            "./assets/resources/astra-militarum-bkg.png",
            "./assets/resources/black-templars-bkg.png",
            "./assets/resources/blood-angels-bkg.png",
            "./assets/resources/blood-ravens-bkg.png",
            "./assets/resources/chaos-daemons-bkg.png",
            "./assets/resources/chaos-space-marines-bkg.png",
            "./assets/resources/dark-angels-bkg.png",
            "./assets/resources/death-guard-bkg.png",
            "./assets/resources/deathwatch-bkg.png",
            "./assets/resources/drukhari-bkg.png",
            "./assets/resources/genestealer-cults-bkg.png",
            "./assets/resources/grey-knights-bkg.png",
            "./assets/resources/imperial-fists-bkg.png",
            "./assets/resources/inquisition-bkg.png",
            "./assets/resources/necrons-bkg.png",
            "./assets/resources/orks-bkg.png",
            "./assets/resources/space-wolves-bkg.png",
            "./assets/resources/tau-bkg.png",
            "./assets/resources/thousand-sons-bkg.png",
            "./assets/resources/tyranids-bkg.png",
            "./assets/resources/world-eaters-bkg.png"
          ]
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2fe3bf31-db8b-4049-a6da-5fe5002ecdc7"
      }
    }
  }
}
