{
  "expo": {
    "name": "appdeptus",
    "slug": "appdeptus",
    "version": "0.0.1",
    "scheme": "com.appdeptus",
    "orientation": "portrait",
    "icon": "./assets/resources/icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/resources/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#18212F"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.appdeptus"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/resources/adaptive-icon.png",
        "backgroundColor": "#18212F"
      },
      "package": "com.appdeptus"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/resources/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/AmericanText.ttf"]
        }
      ],
      [
        "expo-asset",
        {
          "assets": [
            "./assets/resources/splash.png",
            "./assets/resources/bkg/adeptasororitas.png",
            "./assets/resources/bkg/adeptusastartes.png",
            "./assets/resources/bkg/adeptusmechanicus.png",
            "./assets/resources/bkg/aeldari.png",
            "./assets/resources/bkg/armybuilder.png",
            "./assets/resources/bkg/astramilitarum.png",
            "./assets/resources/bkg/blacktemplars.png",
            "./assets/resources/bkg/bloodangels.png",
            "./assets/resources/bkg/bloodravens.png",
            "./assets/resources/bkg/chaosdaemons.png",
            "./assets/resources/bkg/chaosspacemarines.png",
            "./assets/resources/bkg/darkangels.png",
            "./assets/resources/bkg/deathguard.png",
            "./assets/resources/bkg/deathwatch.png",
            "./assets/resources/bkg/drukhari.png",
            "./assets/resources/bkg/genestealercults.png",
            "./assets/resources/bkg/greyknights.png",
            "./assets/resources/bkg/imperialfists.png",
            "./assets/resources/bkg/inquisition.png",
            "./assets/resources/bkg/leviathan.jpg",
            "./assets/resources/bkg/necrons.png",
            "./assets/resources/bkg/orks.png",
            "./assets/resources/bkg/spacewolves.png",
            "./assets/resources/bkg/tau.png",
            "./assets/resources/bkg/thousandsons.png",
            "./assets/resources/bkg/tyranids.png",
            "./assets/resources/bkg/worldeaters.png",
            "./assets/resources/images/40klogo.png"
          ]
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2fe3bf31-db8b-4049-a6da-5fe5002ecdc7"
      }
    }
  }
}
