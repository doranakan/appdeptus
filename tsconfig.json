{
  "extends": [
    "@tsconfig/strictest",
    "expo/tsconfig.base"
  ],
  "compilerOptions": {
    "checkJs": false,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "appdeptus/*": [
        "./*"
      ]
    },
    "moduleSuffixes": [".android", ".ios", ""],
    "exactOptionalPropertyTypes": false
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "env.d.ts",
    "app.config.js",
    "tailwind.config.js",
    "nativewind-env.d.ts"
  ]
}